	<queryString>
		<![CDATA[SELECT * FROM itememprestimo
inner JOIN emprestimo ON itememprestimo.emprestimo_id = emprestimo.idemprestimo
inner join estadobem  on itememprestimo.estadoBem_id = estadobem.idestadobem
inner join funcionario  on emprestimo.funcionario_id = funcionario.idfuncionario
inner join localidade  on funcionario.localidade_id = localidade.idlocalidade
inner join equipamento on itememprestimo.equipamento_patrimonio = equipamento.patrimonio
inner join modelo on equipamento.modelo_id = modelo.idmodelo
inner join categoria on modelo.categoria_id = categoria.idcategoria
WHERE emprestimo.idemprestimo = 80]]>
	</queryString>
